{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./app/components/*"
      ],
      "components": [
        "./app/components"
      ],
      "shared/*": [
        "./app/shared/*"
      ],
      "shared": [
        "./app/shared"
      ]
    }
  }
}